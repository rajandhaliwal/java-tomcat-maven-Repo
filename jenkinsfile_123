pipeline{
 
  agent any
  stages {
   
     stage ('Build a maven project'){
	        steps{
			   bat 'mvn clean package'
			   
			      }
	         post{
			 success{
			     echo 'Now Archiving.....'
				 archiveArtifacts: '**/*.war
			 
			        }
			     }
	 
	 
	                             }
  
  
  
         }




}
